<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="notice">
	<select id="selectNoticeMain" resultType="notice">
	<![CDATA[
		select 
			no,
			title,
			reg_date
		from 
			(select 
				* 
			from  
				notice 
			order by 	
				reg_date desc ) 
		where rownum <= 5
	]]>
	</select>
	
	
</mapper>